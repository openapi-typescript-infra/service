{
  "name": "@openapi-typescript-infra/service",
  "version": "0.0.0",
  "description": "An opinionated framework for building configuration driven services - web, api, or  ob. Uses OpenAPI, pino logging, express, confit, Typescript and vitest.",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "types": "./build/index.d.ts"
    },
    "./telemetry": {
      "import": "./build/telemetry/index.js",
      "types": "./build/telemetry/index.d.ts"
    },
    "./Makefile": "./Makefile"
  },
  "type": "module",
  "scripts": {
    "test": "vitest",
    "lint": "eslint .",
    "build": "tsc -p tsconfig.build.json && yarn dlx glob-chmod 755 build/bin/*",
    "watch": "tsc -p tsconfig.json -w --preserveWatchOutput",
    "clean": "npx rimraf ./build",
    "prepublishOnly": "yarn build",
    "postinstall": "cpconfig"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openapi-typescript-infra/service.git"
  },
  "bin": {
    "start-service": "./build/bin/start-service.js"
  },
  "config": {
    "cpconfig": "@openapi-typescript-infra/cpconfig"
  },
  "engines": {
    "node": ">=22"
  },
  "keywords": [
    "service",
    "openapi",
    "express",
    "confit",
    "babel",
    "typescript",
    "vitest"
  ],
  "author": "developers@pyralis.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/openapi-typescript-infra/service/issues"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/exec",
        {
          "publishCmd": "yarn dlx pinst --disable"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "homepage": "https://github.com/openapi-typescript-infra/service#readme",
  "dependencies": {
    "@godaddy/terminus": "^4.12.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.69.0",
    "@opentelemetry/exporter-prometheus": "^0.212.0",
    "@opentelemetry/instrumentation-dns": "^0.54.0",
    "@opentelemetry/instrumentation-express": "^0.59.0",
    "@opentelemetry/instrumentation-generic-pool": "^0.54.0",
    "@opentelemetry/instrumentation-graphql": "^0.58.0",
    "@opentelemetry/instrumentation-http": "^0.212.0",
    "@opentelemetry/instrumentation-net": "^0.55.0",
    "@opentelemetry/instrumentation-pg": "^0.63.0",
    "@opentelemetry/instrumentation-pino": "^0.57.0",
    "@opentelemetry/instrumentation-redis": "^0.59.0",
    "@opentelemetry/instrumentation-undici": "^0.21.0",
    "@opentelemetry/resource-detector-container": "^0.8.2",
    "@opentelemetry/resource-detector-gcp": "^0.46.0",
    "@opentelemetry/sdk-node": "^0.212.0",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "@sesamecare-oss/confit": "^2.2.1",
    "@sesamecare-oss/opentelemetry-node-metrics": "^2.0.1",
    "ajv": "^8.18.0",
    "clean-stack": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "express-openapi-validator": "^5.6.2",
    "glob": "^13.0.3",
    "import-in-the-middle": "^2.0.6",
    "minimist": "^1.2.8",
    "moderndash": "^4.0.0",
    "opentelemetry-resource-detector-sync-api": "^0.30.0",
    "pino": "^10.3.1",
    "read-package-up": "^12.0.0",
    "request-ip": "^3.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@openapi-typescript-infra/cpconfig": "^1.1.0",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/github": "^12.0.6",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/minimist": "^1.2.5",
    "@types/node": "^24.10.13",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "cpconfig": "^1.4.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "pino-pretty": "^13.1.3",
    "pinst": "^3.0.0",
    "prettier": "^3.8.1",
    "supertest": "^7.2.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "resolutions": {
    "qs": "^6.11.0"
  },
  "packageManager": "yarn@3.2.3"
}
